{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended", 
    ":dependencyDashboard"
  ],
  "baseBranches": [
    "dev"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "lockFileMaintenance",
        "rollback",
        "bump"
      ],
      "automerge": true,
      "ignoreTests": true
    }
  ]
}
