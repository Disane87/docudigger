{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "conventionalCommits.scopes": [
    "Gathering",
    "debugging",
    "selectors",
    "renovate",
    "logging",
    "ci",
    "docs",
    "cli",
    "logs",
    "lint",
    "docker",
    "files",
    "build",
    "package",
    "deps"
  ],
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "npm.exclude": "**/@(vendor|node_modules|bower_components|dist|static)/**"
}
