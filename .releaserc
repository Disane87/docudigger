{
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/release-notes-generator",
      {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES",
          "BREAKING"
        ],
        "writerOpts": {
          "groupBy": "type",
          "commitGroupsSort": [
            "feat",
            "fix",
            "perf",
            "docs"
          ],
          "commitsSort": "header"
        },
        "linkCompare": true,
        "linkReferences": true,
        "presetConfig": {
          "types": [
            {
              "type": "build",
              "section": "ü¶ä CI/CD",
              "hidden": true
            },
            {
              "type": "chore",
              "section": "üßπ Other",
              "hidden": true
            },
            {
              "type": "ci",
              "section": "ü¶ä CI/CD",
              "hidden": true
            },
            {
              "type": "docs",
              "section": "üìî Docs",
              "hidden": false
            },
            {
              "type": "example",
              "section": "üìù Examples",
              "hidden": false
            },
            {
              "type": "feat",
              "section": "üöÄ Features",
              "hidden": false
            },
            {
              "type": "fix",
              "section": "üõ†Ô∏è Fixes",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "‚è© Performance"
            },
            {
              "type": "refactor",
              "section": ":scissors: Refactor",
              "hidden": true
            },
            {
              "type": "revert",
              "section": "üôÖ‚ÄçÔ∏è Reverts"
            },
            {
              "type": "style",
              "section": "üíà Style",
              "hidden": true
            },
            {
              "type": "test",
              "section": "üß™ Tests",
              "hidden": true
            }
          ]
        }
      }
    ],
    "@semantic-release/changelog",
    [
      "@semantic-release/npm",
      {
        "tarballDir": "./archive/"
      }
    ],
    [
      "@semantic-release/git",
      {
        "message": "chore(release): üì¢ ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}",
        "assets": [
          "CHANGELOG.md",
          "package.json",
          "package-lock.json",
          "npm-shrinkwrap.json",
          "README.md"
        ]
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          "./archive/*.tgz"
        ],
        "fail": true
      }
    ],
    [
      "@codedependant/semantic-release-docker",
      {
        "dockerTags": [
          "{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
          "{{major}}-{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
          "{{major}}.{{minor}}-{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
          "{{version}}"
        ],
        "dockerRegistry": "ghcr.io",
        "dockerFile": "dockerfile",
        "dockerLogin": false,
        "dockerArgs": {
          "DOCUDIGGER_VERSION": "{{next.version}}"
        }
      }
    ]
  ],
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": true,
      "channel": "dev"
    }
  ]
}