{
  "plugins": [
    "@semantic-release/commit-analyzer",
    ["@semantic-release/release-notes-generator", {
      "writerOpts": {
        "groupBy": "type",
        "commitGroupsSort": [ "feat", "fix", "perf", "docs" ],
        "commitsSort": "header"
      },
      "linkCompare": true,
      "linkReferences": true,
      "presetConfig": {
        "types": [
          { "type": "build", "section": "ğŸ¦Š CI/CD", "hidden": true },
          { "type": "chore", "section": "ğŸ§¹ Other", "hidden": true },
          { "type": "ci", "section": "ğŸ¦Š CI/CD", "hidden": true },
          { "type": "docs", "section": "ğŸ“” Docs", "hidden": false },
          { "type": "example", "section": "ğŸ“ Examples", "hidden": false },
          { "type": "feat", "section": "ğŸš€ Features", "hidden": false },
          { "type": "fix", "section": "ğŸ› ï¸ Fixes", "hidden": false },
          { "type": "perf", "section": "â© Performance" },
          {
            "type": "refactor",
            "section": ":scissors: Refactor",
            "hidden": true
          },
          { "type": "revert", "section": "ğŸ™…â€ï¸ Reverts" },
          { "type": "style", "section": "ğŸ’ˆ Style", "hidden": true },
          { "type": "test", "section": "ğŸ§ª Tests", "hidden": true }
        ]
      }
    }],
    "@semantic-release/changelog",
    [
      "@semantic-release/npm",
      {
        "tarballDir": "./archive/"
      }
    ],
    [
      "@semantic-release/git",
      {
        "message": "chore(release): ğŸ“¢ ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}",
        "assets": ["CHANGELOG.md", "package.json", "package-lock.json", "npm-shrinkwrap.json", "README.md"]
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": ["./archive/*.tgz"],
        "fail": true
      }
    ]
  ],
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": true,
      "channel": "dev"
    }
  ]
}