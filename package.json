{
  "name": "document-scraper",
  "version": "0.0.0",
  "description": "Universal and extensible document scraper to get documents from pages (like customer portals etc.)",
  "author": "Marco Franke @Disane87",
  "bin": {
    "document-scraper": "./bin/run"
  },
  "homepage": "https://github.com/Disane87/document-scraper",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Disane87/document-scraper",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.21.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.8",
    "luxon": "^3.1.1",
    "puppeteer": "^19.4.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@oclif/test": "^2.2.15",
    "@saithodev/semantic-release-gitea": "^2.1.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4",
    "@types/luxon": "^3.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.15",
    "@types/puppeteer": "^7.0.4",
    "@types/winston": "^2.4.4",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.29.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "mocha": "^10",
    "oclif": "^3",
    "semantic-release": "^19.0.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "document-scraper",
    "dirname": "document-scraper",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "scrape": {
        "description": "Say hello to the world and others"
      }
    },
    "hooks": {
      "prerun": "./dist/hooks/prerun/prerun"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "semantic-release": "env-cmd npx semantic-release",
    "semantic-release:dry": "npm run lint && npm run build && npm run semantic-release -- -d --no-ci",
    "semantic-release:run": "npm run lint && npm run build && cross-env HUSKY=0  npm run semantic-release -- --no-ci",
    "semantic-release:ci": "npm run lint && npm run build && cross-env HUSKY=0  npm run semantic-release",
    "postversion": "copyfiles package.json npm-shrinkwrap.json ..",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Disane87/document-scraper/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
