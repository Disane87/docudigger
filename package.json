{
  "name": "@docudigger/core",
  "version": "0.0.0",
  "description": "Universal and extensible document scraper to get documents from pages (like customer portals etc.)",
  "author": "Marco Franke @Disane87",
  "bin": {
    "docudigger": "./bin/run"
  },
  "homepage": "https://github.com/Disane87/docudigger",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Disane87/docudigger",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-commands": "^2.2.16",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.1.5",
    "luxon": "^3.3.0",
    "node-cron": "^3.0.2",
    "puppeteer": "^20.7.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.6",
    "@oclif/test": "^2.3.25",
    "@saithodev/semantic-release-gitea": "^2.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/chai": "^4",
    "@types/luxon": "^3.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.2",
    "@types/node-cron": "^3.0.8",
    "@types/puppeteer": "^7.0.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "mocha": "^10",
    "oclif": "^3.9.1",
    "semantic-release": "^21.0.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.5"
  },
  "oclif": {
    "bin": "docudigger",
    "dirname": "docudigger",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-commands"
    ],
    "topicSeparator": " ",
    "topics": {
      "scrape": {
        "description": "Scrape tha interwebz"
      }
    },
    "hooks": {
      "prerun": "./dist/hooks/prerun/prerun"
    }
  },
  "scripts": {
    "postinstall": "ts-node ./scripts/postinstall.ts",
    "start": "cd ./bin && dev scrape amazon",
    "build": "npm run clean && tsc -b",
    "lint": "eslint ./src --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm shrinkwrap && npm run build && npx oclif manifest && npx oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "npx oclif readme && git add README.md",
    "semantic-release": "env-cmd npx semantic-release",
    "semantic-release:dry": "npm run lint && npm run build && npm run semantic-release -- -d --no-ci",
    "semantic-release:run": "npm run lint && npm run build && cross-env HUSKY=0  npm run semantic-release -- --no-ci",
    "semantic-release:ci": "npm run lint && npm run build && cross-env HUSKY=0  npm run semantic-release",
    "postsemantic-release": "npm run clean",
    "postbuild": "copyfiles package.json ./dist",
    "prepare": "echo husky install",
    "clean": "shx rm -rf dist"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Disane87/docudigger/issues",
  "keywords": [
    "Documents scraper invoices"
  ],
  "types": "dist/index.d.ts"
}
