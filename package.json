{
  "name": "@disane-dev/amazon-invoice-scraper",
  "repository": {
    "type": "git",
    "url": "https://repo.disane.dev/Disane/amazon-invoice-scraper.git"
  },
  "bugs": {
    "url": "https://repo.disane.dev/Disane/amazon-invoice-scraper/issues"
  },
  "version": "1.0.0-dev.1",
  "description": "Amazon scraper for getting invoices automagically as pdf (useful for taxes or DMS)",
  "homepage": "https://repo.disane.dev/Disane/amazon-invoice-scraper#readme",
  "main": "dist/index.ts",
  "scripts": {
    "prestart": "npx tsc",
    "start": "node --inspect dist/index.js",
    "lint": "eslint . --ext .ts",
    "preinstall": "npx husky install",
    "copyfiles": "copyfiles package.json .env_example *.md ./dist/",
    "semantic-release": "env-cmd npx semantic-release",
    "semantic-release:dry": "npm run lint && npm run build && npm run semantic-release -- -d --no-ci",
    "semantic-release:run": "npm run lint && npm run build && cross-env HUSKY=0  npm run semantic-release -- --no-ci",
    "semantic-release:ci": "npm run lint && cross-env HUSKY=0  npm run build && npm run semantic-release",
    "prebuild": "rimraf ./dist",
    "postversion": "copyfiles package.json npm-shrinkwrap.json ..",
    "build": "tsc",
    "postbuild": "npm run copyfiles",
    "prepublish": "npm run build && npm run copyfiles"
  },
  "author": "Marco Franke <mfranke87@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@saithodev/semantic-release-gitea": "^2.1.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/commander": "^2.12.2",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.9",
    "@types/prompts": "^2.4.1",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "semantic-release": "^19.0.5",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@commitlint/cli": "^17.3.0",
    "commander": "^9.4.1",
    "config-decorators": "^0.2.0",
    "copyfiles": "^2.4.1",
    "luxon": "^3.1.0",
    "prompts": "^2.4.2",
    "puppeteer": "^19.3.0",
    "puppeteer-extra": "^3.3.4",
    "winston": "^3.8.2"
  },
  "engines": {
    "npm": ">=9.1.2",
    "node": ">=18.12.1"
  },
  "bin": {
    "amazon-invoice": "./index.js"
  }
}
